{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["svelte", "vite/client"],
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"]
    }
  }
}
